                        <li><a runat="server" href="~/Account/Registration.aspx">Sign up</a></li>
                        <li><a runat="server" href="~/Account/User_history_details.aspx">User Details</a></li><%--This is temporary--%>
<%--                        <li><a runat="server" href="~/Account/ManageTicket.aspx">Tickets</a></li><%--This is temporary--%>
                        <li><a runat="server" href="~/Account/Manage_Tickets_events.aspx">ManageTickets</a></li>
                       
                    </ul>
                    
                </div>
            </div>
